              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "results": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "toolCallId": {
                            "type": "string"
                          },
                          "result": {
                            "type": "object",
                            "properties": {
                              "caller": {
                                "type": "string",
                                "description": "Phone number of the caller (your customer)"
